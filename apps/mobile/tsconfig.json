{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "lib": ["es2017", "es2018", "es2019", "dom"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  }
}
